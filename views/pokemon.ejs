<h1>You've reached the Favorites Page</h1>



<% if(pokemon.length === 0) { %>
    <h4>No favorites here! Go catch some!</h4>
<% } else { %>
    <% pokemon.forEach((poke) => { %>
        <h2><%= poke.name %></h2>
    <% })} %> 
